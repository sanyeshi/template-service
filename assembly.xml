<?xml version="1.0"?>
<assembly>
	<id>template-service</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>${project.build.directory}/conf</directory>
			<outputDirectory>conf</outputDirectory>
		</fileSet>
	</fileSets>

	<files>
		<file>
			<source>${project.build.directory}/${bulid.name}.jar
			</source>
			<outputDirectory />
			<filtered>false</filtered>
		</file>
	</files>

	<dependencySets>
		<dependencySet>
			<outputDirectory>lib</outputDirectory>
		</dependencySet>
	</dependencySets>


</assembly>
